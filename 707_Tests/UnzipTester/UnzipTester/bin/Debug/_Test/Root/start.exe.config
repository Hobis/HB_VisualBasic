<?xml version="1.0" encoding="utf-8"?>
<configuration>

  <!-- Runtime Setting -->
  <startup>
    <supportedRuntime version="v2.0.50727"/>
    <supportedRuntime version="v4.0"/>
  </startup>

  <appSettings>
    <!-- 어플리케이션 인스턴스 키 -->
    <add key="AppInstKey" value="AMT-APPKET-YE3D1"/>

    <!-- 타이틀 설정 -->
    <add key="Title" value="EFrameNewEdge-Test"/>

    <!-- 타이틀 아이콘 -->
    <add key="Icon" value="start.ico"/>

    <!-- 시작점 frame.swf -->
    <add key="EntryPoint" value="disk1/frame.swf?xml=index_cd_ebook.xml"/>


    <!-- 테스트 베이스 여부 -->
    <add key="IsTest" value="False"/>
  </appSettings>

  <!-- App Settings -->
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <probing privatePath="dlls"/>
    </assemblyBinding>
  </runtime>

</configuration>
